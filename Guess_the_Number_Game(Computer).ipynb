      "source": [
        "import random\n",
        "\n",
        "print(\"沁ｯ Welcome to Project 2: Guess the Number Game! 洟暴")\n",
        "print(\"I'm thinking of a number between 1 and 5... Can you read my mind? 洫\")\n",
        "print(\"Let the battle of wits begin! 汳･\")\n",
        "\n",
        "secret_number = random.randint(1, 5)\n",
        "guess_count = 0\n",
        "\n",
        "messages = [\n",
        "    \"You're getting warmer! 沐･\",\n",
        "    \"Oops, colder... 笶ｸ十",\n",
        "    \"I can feel the suspense! 沽ｱ\",\n",
        "    \"Are you a psychic? 洫吮坂凰ｸ十",\n",
        "    \"Hmm... getting interesting! 洟能"\n",
        "]\n",
        "\n",
        "while True:\n",
        "    try:\n",
        "        guess = int(input(\"Enter your guess (1-5): \"))\n",
        "        if guess < 1 or guess > 5:\n",
        "            print(\"Out of bounds! Please guess a number between 1 and 5. 泅ｫ\")  # Fixed range\n",
        "            continue\n",
        "    except ValueError:\n",
        "        print(\"Invalid input! Please enter a number. 泅ｨ\")\n",
        "        continue\n",
        "\n",
        "    guess_count += 1\n",
        "\n",
        "    if guess < secret_number:\n",
        "        print(f\"Too low! 沒 {random.choice(messages)}\")  # Simplified feedback\n",
        "    elif guess > secret_number:\n",
        "        print(f\"Too high! 沒 {random.choice(messages)}\")  # Simplified feedback\n",
        "    else:\n",
        "        print(f\"沁 Woohoo! You guessed the number {secret_number} in {guess_count} attempts! 沛\")\n",
        "        if guess_count <= 5:\n",
        "            print(\"You're a genius! 洫\")\n",
        "        elif guess_count <= 10:\n",
        "            print(\"Great job! That was impressive! 箝申")\n",
        "        else:\n",
        "            print(\"You did it! Persistence always wins! 汳ｪ\")\n",
        "        break\n",
        "\n",
        "print(\"Thanks for playing! You're a true mind-reader... or just lucky! 沚\")"
      ]
